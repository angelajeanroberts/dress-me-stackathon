  "source": "pragma solidity ^0.4.24;\n\ncontract PayTip {\n    address public client;\n    address public stylist;\n    uint public tip;\n    \n    constructor() public {\n        stylist = msg.sender;\n    }\n    \n    function setTip (uint amount) public {\n        tip = amount * 1; //1 ether\n        client = msg.sender;\n    }\n    \n    function finishPayment() public {\n        stylist.transfer(address(this).balance);\n    }\n    \n}",
